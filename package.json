{
  "name": "lusamine-data-extractor",
  "version": "1.0.0",
  "description": "Tool to migrate data from json files to sqlite db",
  "type": "commonjs",
  "files": [
    "bin"
  ],
  "bin": {
    "lde": "./bin/index.js"
  },
  "scripts": {
    "build": "tsc -p .",
    "test": "echo 'Tests not set up!'",
    "pretty": "prettier --write \"{,!(node_modules|bin|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
    "lint": "eslint \"{,!(node_modules|bin)/**/}*.{js,ts}\" --fix",
    "prepare": "husky install"
  },
  "lint-staged": {
    "{,src}/**/*.{js,ts}": [
      "npm run pretty",
      "npm run lint"
    ]
  },
  "author": "Bakuzan",
  "license": "ISC",
  "dependencies": {
    "better-sqlite3": "^7.5.1",
    "chalk": "^5.0.1",
    "commander": "^9.2.0",
    "dotenv": "^16.0.0",
    "figlet": "^1.5.2"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.5.0",
    "@types/figlet": "^1.5.4",
    "@types/node": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "typescript": "^4.6.4"
  }
}
